{
  "manifest_version": 3,
  "name": "Reload Specific URL",
  "version": "1.0",
  "description": "A simple extension to reload a specific URL when a button is clicked.",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}